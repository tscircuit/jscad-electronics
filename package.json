{
  "name": "jscad-electronics",
  "type": "module",
  "version": "0.0.69",
  "main": "./dist/index.js",
  "exports": {
    ".": {
      "import": "./dist/index.js"
    },
    "./vanilla": {
      "import": "./dist/vanilla.js"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/tscircuit/jscad-electronics"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "start": "cosmos",
    "build": "tsup --config tsup.config.ts",
    "vercel-build": "bun run build:site",
    "format": "biome format --write .",
    "format:check": "biome format .",
    "build:site": "cosmos-export",
    "test:vanilla": "bun run build && node tests/vanilla-smoke.mjs",
    "test": "bun test"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.3",
    "@tscircuit/footprinter": "^0.0.242",
    "@types/react": "19",
    "@types/react-dom": "19",
    "@types/three": "^0.179.0",
    "bun-types": "^1.2.20",
    "jscad-fiber": "^0.0.85",
    "jscad-to-gltf": "^0.0.4",
    "looks-same": "^10.0.1",
    "poppygl": "^0.0.15",
    "react-cosmos": "7.0.0",
    "react-cosmos-plugin-vite": "7.0.0",
    "tsup": "^8.3.0",
    "vite": "^7.1.2",
    "vite-tsconfig-paths": "^5.0.1"
  },
  "peerDependencies": {
    "three": "^0.179.1",
    "@jscad/modeling": "^2.12.5",
    "jscad-fiber": "^0.0.85",
    "@tscircuit/footprinter": "*",
    "circuit-json": "^0.0.232",
    "react": "19.1.0",
    "react-dom": "19.1.0"
  },
  "peerDependenciesMeta": {
    "jscad-fiber": {
      "optional": true
    }
  }
}
